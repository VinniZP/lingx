{
  "name": "@localeflow/cli",
  "version": "0.0.0",
  "description": "Localeflow CLI tool for translation management",
  "private": true,
  "type": "module",
  "bin": {
    "lf": "./bin/lf.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@localeflow/config": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.28.0",
    "@types/inquirer": "^9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@formatjs/icu-messageformat-parser": "^3.2.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "glob": "^13.0.0",
    "inquirer": "^13.1.0",
    "js-yaml": "^4.1.1",
    "ora": "^9.0.0"
  }
}
